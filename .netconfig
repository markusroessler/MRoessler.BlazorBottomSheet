[ReportGenerator]
    reports = "**/TestResults/*/coverage.cobertura.xml;coverage/cobertura-coverage.xml" # REQUIRED # The coverage reports that should be parsed (separated by semicolon). Globbing is supported.
    reporttypes = "Html;Badges;MarkdownSummaryGithub" # The output formats and scope (separated by semicolon) Values: Badges, Clover, Cobertura, CsvSummary, Html, HtmlChart, HtmlInline, HtmlInline_AzurePipelines, HtmlInline_AzurePipelines_Dark, HtmlSummary, JsonSummary, Latex, LatexSummary, lcov, MarkdownSummary, MHtml, PngChart, SonarQube, TeamCitySummary, TextSummary, Xml, XmlSummary
    targetdir = "TestResults/coverage-report" # REQUIRED # The directory where the generated report should be saved.
    historydir = "TestResults/coverage-history" # Optional directory for storing persistent coverage information. Can be used in future reports to show coverage evolution.
    