    "version": "0.2.0",
    "configurations": [
        {
            "name": "Sample.WebApp",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/Sample.WebApp/Sample.WebApp.csproj",
        },
        {
            "name": "Sample.WebApp (Attach)",
            "type": "coreclr",
            "request": "attach",
            "processName": "Sample.WebApp.exe",
        },
        {
            "name": "Sample.Wasm",
            "type": "blazorwasm",
            "request": "launch",
            "cwd": "${workspaceFolder}/Sample.Wasm",
            "browser": "chrome",
        },
